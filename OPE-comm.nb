}], "Text",
 CellChangeTimes->{{3.711464295801897*^9, 3.711464476028565*^9}, {
  3.711464590463029*^9, 3.711464718744277*^9}, {3.711465239601552*^9, 
  3.7114652620843782`*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"OPEtoCommutator", "[", 
    RowBox[{"OPE_", ",", "OPRList_"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
      "ModeSummand", ",", "GlobalPartSummand", ",", "GlobalPart", ",", 
       "ModeExp"}], "}"}], ",", 
     RowBox[{
      RowBox[{"$Assumptions", "=", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{
          RowBox[{"{", 
           RowBox[{"m", ",", "n"}], "}"}], "\[Element]", " ", "Integers"}], 
         ",", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{"m", ",", "n"}], "}"}], "\[Element]", " ", "Reals"}]}], 
        "}"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"ModeExp", "=", 
       RowBox[{"{", "}"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"For", "[", 
       RowBox[{
        RowBox[{"j", "=", "0"}], ",", 
        RowBox[{"j", "\[LessEqual]", "4"}], ",", 
        RowBox[{"j", "++"}], ",", 
        RowBox[{"ModeExp", "=", 
         RowBox[{"Union", "[", 
          RowBox[{"ModeExp", ",", 
           RowBox[{"Table", "[", "  ", 
            RowBox[{
             RowBox[{
              RowBox[{
               SubscriptBox["\[PartialD]", 
                RowBox[{"{", 
                 RowBox[{"w", ",", "j"}], "}"}]], 
               RowBox[{"(", 
                RowBox[{
                 RowBox[{"OPRList", "[", 
                  RowBox[{"[", 
                   RowBox[{"i", ",", "1"}], "]"}], "]"}], "@", "w"}], ")"}]}],
               "\[Rule]", " ", 
              RowBox[{
               SubscriptBox["\[PartialD]", 
                RowBox[{"{", 
                 RowBox[{"w", ",", "j"}], "}"}]], " ", 
               RowBox[{"(", 
                FractionBox[
                 RowBox[{" ", 
                  SubscriptBox[
                   RowBox[{"OPRList", "[", 
                    RowBox[{"[", 
                    RowBox[{"i", ",", "1"}], "]"}], "]"}], "q"]}], 
                 SuperscriptBox["w", 
                  RowBox[{"q", "+", 
                   RowBox[{"OPRList", "[", 
                    RowBox[{"[", 
                    RowBox[{"i", ",", "2"}], "]"}], "]"}]}]]], ")"}]}]}], 
             "  ", ",", 
             RowBox[{"{", 
              RowBox[{"i", ",", "1", ",", 
               RowBox[{"Length", "[", "OPRList", "]"}]}], "}"}]}], "]"}]}], 
          "]"}]}]}], "]"}], ";", "\[IndentingNewLine]", 
      RowBox[{"ModeSummand", ":=", 
       RowBox[{
        RowBox[{
         RowBox[{"(", 
          RowBox[{
           RowBox[{"(", 
            RowBox[{
             RowBox[{
              RowBox[{"Residue", "[", 
               RowBox[{
                RowBox[{
                 SuperscriptBox["z", 
                  RowBox[{"m", "+", 
                   RowBox[{"OPRList", "[", 
                    RowBox[{"[", 
                    RowBox[{"1", ",", "2"}], "]"}], "]"}], "-", "1"}]], 
                 SuperscriptBox["w", 
                  RowBox[{"n", "+", 
                   RowBox[{"OPRList", "[", 
                    RowBox[{"[", 
                    RowBox[{"2", ",", "2"}], "]"}], "]"}], "-", "1"}]], 
                 "OPE"}], ",", 
                RowBox[{"{", 
                 RowBox[{"z", ",", "w"}], "}"}]}], "]"}], "//", "Simplify"}], 
             "//", "Distribute"}], ")"}], "//.", "ModeExp"}], ")"}], "//.", 
         RowBox[{"{", 
          RowBox[{
           SuperscriptBox["w", "x_"], "\[RuleDelayed]", 
           RowBox[{"KroneckerDelta", "[", 
            RowBox[{
             RowBox[{"x", "+", "1"}], ",", "0"}], "]"}]}], "}"}]}], "//", 
        "Simplify"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"GlobalPartSummand", "=", 
       RowBox[{"ModeSummand", "-", 
        RowBox[{"c", " ", 
         RowBox[{"Coefficient", "[", 
          RowBox[{"ModeSummand", ",", "c"}], "]"}]}]}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"GlobalPart", "=", 
       RowBox[{
        UnderoverscriptBox["\[Sum]", 
         RowBox[{"q", "=", 
          RowBox[{"-", "\[Infinity]"}]}], "\[Infinity]"], 
        "GlobalPartSummand"}]}], ";", 
      RowBox[{"GlobalPart", "+", 
       RowBox[{"c", " ", 
        RowBox[{"Coefficient", "[", 
         RowBox[{"ModeSummand", ",", "c"}], "]"}]}]}]}]}], "]"}]}], 
  ";"}]], "Input",
 CellChangeTimes->{{3.6517583834809785`*^9, 3.6517585378034134`*^9}, 
